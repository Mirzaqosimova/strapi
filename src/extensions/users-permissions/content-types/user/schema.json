{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "educations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::education.education",
      "mappedBy": "users"
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "MALE",
        "FEMALE"
      ]
    },
    "description": {
      "type": "text"
    },
    "passportNumber": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    },
    "achievements": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::achievement.achievement",
      "mappedBy": "users_permissions_user"
    },
    "attachment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::attachment.attachment",
      "inversedBy": "users"
    },
    "complaints": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::complaint.complaint",
      "mappedBy": "users_permissions_user"
    },
    "consultants": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::consultant.consultant",
      "mappedBy": "users_permissions_user"
    },
    "occupations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::occupation.occupation",
      "inversedBy": "users_permissions_users"
    },
    "publications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::publication.publication",
      "mappedBy": "users_permissions_user"
    },
    "devices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::device.device"
    }
  }
}
